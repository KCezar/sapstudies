{
  "name": "commons",
  "version": "1.0.0",
  "private": true,
  "devDependencies": {
    "@ui5/cli": "^2.9.3",
    "karma": "^6.1.1",
    "karma-chrome-launcher": "^3.1.0",
    "karma-cli": "^2.0.0",
    "karma-ui5": "^2.3.3",
    "ui5-middleware-livereload": "^0.5.1",
    "mbt": "^1.0.15",
    "rimraf": "3.0.0",
    "@sap/ui5-builder-webide-extension": "1.0.x",
    "ui5-task-zipper": "^0.3.1",
    "@sap/ux-ui5-tooling": "1",
    "bestzip": "2.1.4"
  },
  "ui5": {
    "dependencies": [
      "ui5-middleware-livereload",
      "@sap/ui5-builder-webide-extension",
      "ui5-task-zipper",
      "mbt",
      "@sap/ux-ui5-tooling"
    ]
  },
  "scripts": {
    "build": "ui5 build --clean-dest --dest dist && cp ./xs-app.json dist",
    "build:mta": "rimraf resources mta_archives && mbt build --mtar archive",
    "deploy": "cf deploy mta_archives/archive.mtar",
    "build:cf": "ui5 build preload --clean-dest --config ui5-deploy.yaml --include-task=generateManifestBundle generateCachebusterInfo && cp ./xs-app.json dist/resources/route/test/commons && npm run zip",
    "undeploy": "cf undeploy commons --delete-services --delete-service-keys",
    "start": "ui5 serve --open test-resources/route/test/commons/Example.html",
    "testsuite": "ui5 serve --open test-resources/route/test/commons/qunit/testsuite.qunit.html",
    "test": "karma start --browsers=ChromeHeadless --singleRun=true",
    "zip": "cd 'dist/resources/route/test/commons' && npx bestzip '../../../../routetestcommons.zip' *"
  },
  "license": "UNLICENSED"
}
